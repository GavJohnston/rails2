<h1>Listing clinics</h1>
<%= link_to 'New clinic', new_clinic_path %> 
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Clinic Name</th>
      <th scope="col">Clinic Address</th>

    </tr>
  </thead>
  <tbody>
 <% @clinics.each do |clinic| %>
 <tr>
 <td><%= clinic.clinicname %></td>
 <td><%= clinic.clinicaddress %></td>

   






 <td><%= link_to 'Show', clinic_path(clinic) %></td>
 <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td> 
 <td><%= link_to 'Destroy', clinic_path(clinic),
 method: :delete,
 data: { confirm: 'Are you sure?' } %></td> 
 </tr>
   </tbody>
 <% end %>
</table> 